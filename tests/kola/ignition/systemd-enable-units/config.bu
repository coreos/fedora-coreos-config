variant: fcos
version: 1.3.0
systemd:
  units:
    - name: touch@.service
      contents: |
        [Service]
        Type=oneshot
        ExecStart=/bin/touch /run/%i
        RemainAfterExit=yes
        [Install]
        WantedBy=multi-user.target
    - name: touch@foo.service
      enabled: true
    - name: podman.socket
      enabled: true
