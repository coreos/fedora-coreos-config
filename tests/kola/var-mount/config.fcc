variant: fcos
version: 1.3.0
storage:
  disks:
    - device: /dev/vda
      partitions:
        - label: var
          size_mib: 0
          start_mib: 5000
      wipe_table: false
  filesystems:
    - device: /dev/disk/by-partlabel/var
      format: xfs
      path: /var
      with_mount_unit: true
